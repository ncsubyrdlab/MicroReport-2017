<?xml version="1.0" encoding="utf-8" ?>
<resources>
    <!-- The application name. Defaults to name specified for the application label -->
    <string name="ga_appName">MicroReport</string>

    <!-- The dispatching period in seconds when Google Play services is
         unavailable. The default period is 1800 seconds or 30 minutes -->
    <integer name="ga_dispatchPeriod">1800</integer>

    <!-- Enable dry run mode. Default is false -->
    <bool name="ga_dryRun">false</bool>

    <!--Session timeout 30 minutes
    <integer name="ga_sessionTimeout">300</integer-->

    <!-- The screen names that will appear in reports -->
    <screenName name="edu.ucsc.sites.microreport.BulletinBoard">
        Good News
    </screenName>
    <screenName name="edu.ucsc.sites.microreport.ReportActivity">
        Submit a Report
    </screenName>
    <screenName name="edu.ucsc.sites.microreport.SurveyActivity">
        Resources
    </screenName>
    <screenName name="edu.ucsc.sites.microreport.RegisterActivity">
        Registration
    </screenName>
    <screenName name="edu.ucsc.sites.microreport.MainActivity">
        Main
    </screenName>
    <screenName name="edu.ucsc.sites.microreport.ConfirmReport">
        Confirm Report
    </screenName>
</resources>